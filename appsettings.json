{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "user_moive_search.middelware.workers.ELK.ElkWorker": "Debug",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  /*for mongo auth with Identity*/
  "AllowedHosts": "*",
  "MongoDbIdentityConfig": {
    "Name": "UserTracker",
    "Host": "localhost",
    "Port": 27017
  },
  /*for elastic search */
  "ELKConfiguration": {
    "Uri": "http://localhost:9200",
    "index": "movie"
  }
}
